 // 3,6,9 게임 
function solution() {
  var answer = [];

  for (var i = 1; i <= 30; i++) {
    if ((i % 10) % 3 == 0 && i % 10 != 0) {
      answer.push('짝!');
    } else if (Math.floor((i / 10) % 3) == 0 && Math.floor(i / 10) != 0) {
      answer.push('짝!');
    } else {
      answer.push(i);
    }
  }
  return answer;
}
.
module.exports = solution;
